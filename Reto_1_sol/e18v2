//JSON
let dato1={
    mes:"Enero",
    dias:"31"
};

// me faltan algunos meses
let arrayDatos = [dato1, {mes:"Febrero",dias:28},{mes:"Marzo",dias:31},{mes:"Abril",dias:30},{mes:"Mayo",dias:31},{mes:"Junio",dias:30},{mes:"Julio",dias:31},{mes:"Agosto",dias:31},{mes:"Septiembre",dias:30}]

let mesNumero = parseInt(prompt("Teclea un mes en n√∫mero"));
alert("Has indicado " + arrayDatos[mesNumero - 1].mes + " que tiene " + arrayDatos[mesNumero - 1]["dias"] + " dias");